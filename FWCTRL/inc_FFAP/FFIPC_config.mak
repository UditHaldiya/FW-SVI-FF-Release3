#Specify whether pass-through (or ISP) HART supports emulation of simulation "jumper"
FFIPC_OPTIONS_SIMJUMPER_EMU = SUPPORTED, NOT_SUPPORTED
FFIPC_SELECTION_SIMJUMPER_EMU = SUPPORTED

#Specify whether pass-through (or ISP) HART supports emulation of write-protect jumper
FFIPC_OPTIONS_WPJUMPER_EMU = SUPPORTED, NOT_SUPPORTED
FFIPC_SELECTION_WPJUMPER_EMU = SUPPORTED
