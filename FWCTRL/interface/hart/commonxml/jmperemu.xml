<HART Version="1.0">
  <Include></Include>
  <Commands>
    <Command CmdNum="170" SubCmdOnly="1">
      <RequestParams />
      <SubCommands>
        <Command Name="Read Emulated FF Simulation Jumper" CmdNum="21" CmdType="Read" AllowedModes="HC_ALLOWED_MODE_ALL" CmdFlags="HC_READ_COMMAND" Description="Read Control Parameters Set">
          <ResponseParams>
            <Param>FF Simulation Enabled flag</Param>
          </ResponseParams>
        </Command>
      </SubCommands>
    </Command>
    <Command CmdNum="171" SubCmdOnly="1">
      <SubCommands>
        <Command CmdNum="21" Name="Emulate FF Simulation Jumper" CmdType="Command" AllowedModes="HC_ALLOWED_MODE_ALL" CmdFlags="HC_COMMAND_COMMAND" Description="Sets the tuning parameters and optionally runs the tuning algorithm">
          <RequestParams>
            <Param>FF Simulation Enabled flag</Param>
          </RequestParams>
          <ResponseParams>
            <Param>FF Simulation Enabled flag</Param>
          </ResponseParams>
        </Command>
      </SubCommands>
    </Command>
    <Command CmdNum="129" SubCmdOnly="1">
      <RequestParams />
      <SubCommands>
        <Command Name="Read HW Lock Emulation Option" CmdNum="21" CmdType="Read" AllowedModes="HC_ALLOWED_MODE_ALL" CmdFlags="HC_READ_COMMAND" Description="Read Control Parameters Set">
          <ResponseParams>
            <Param>Hard Write Lock Emulation</Param>
            <Param>Hard Write Lock Emu TTL</Param>
          </ResponseParams>
        </Command>
      </SubCommands>
    </Command>
    <Command CmdNum="130" SubCmdOnly="1">
      <SubCommands>
        <Command CmdNum="21" Name="Set HW Lock Emulation Option" CmdType="Command" AllowedModes="HC_ALLOWED_MODE_ALL" CmdFlags="HC_COMMAND_COMMAND | HC_FACTORY_COMMAND" Description="Sets the tuning parameters and optionally runs the tuning algorithm">
          <RequestParams>
            <Param>Hard Write Lock Emulation</Param>
            <Param>Hard Write Lock Emu TTL</Param>
          </RequestParams>
          <ResponseParams>
            <Param>Hard Write Lock Emulation</Param>
            <Param>Hard Write Lock Emu TTL</Param>
          </ResponseParams>
        </Command>
      </SubCommands>
    </Command>
  </Commands>
  <Variables>
    <Variable Name="FF Simulation Enabled flag" DataType="H_UCHAR" Description="FF Simulation Enabled">
      <EnumValues v0="FFSim_Off | Off" v1="FFSim_On | On" />
    </Variable>
    <Variable Name="Hard Write Lock Emulation" DataType="H_UCHAR" Description="Hard Write Lock Emulation for testing/development only">
      <EnumValues v0="HWLock_Disabled | Disabled " v1="HWLock_Off | Off" v2="HWLock_On | On" />
    </Variable>
    <Variable Name="Hard Write Lock Emu TTL" DataType="H_UINT4" Description="Hard Write Lock Emulation time to live" Units="ticks"></Variable>
  </Variables>
</HART>