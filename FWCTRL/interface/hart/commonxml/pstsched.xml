  </Commands>
  <Structs>
    <Struct Name="H PST Time Until">
      <Param>PST Days Until Next</Param>
      <Param>PST Minutes Until Next</Param>
    </Struct>
  </Structs>
  <Variables>
    <Variable Name="PST Interval" DataType="H_INT" Description="Schedule adjustment within year" Units="days">
      <Range Model="Standard" MinValue="0" MaxValue="365" />
    </Variable>
    <Variable Name="PST Days Until Next" DataType="H_INT" Description="Schedule adjustment within year" Units="days">
      <Range Model="Standard" MinValue="0" MaxValue="365" />
    </Variable>
    <Variable Name="PST Minutes Until Next" DataType="H_INT" Description="Schedule adjustment within day" Units="minutes">
      <Range Model="Standard" MinValue="0" MaxValue="1439" />
    </Variable>
    <Variable Name="PST Trigger By UI" DataType="H_UCHAR" Description="Whether can be triggered by local UI">
      <EnumValues v0="UIT_Disabled" v1="UIT_Enabled" />
    </Variable>
    <Variable Name="Data Collection Trigger By HART" DataType="H_UCHAR" Description="How to react to HART message trigger">
      <EnumValues v0="HCT_Disabled" v1="HCT_Enabled" />
    </Variable>
    <Variable Name="Data Collection Trigger By AI Input" DataType="H_UCHAR" Description="How to react to physical AI input">
      <EnumValues v0="HCTAI_Disabled" v1="HCTAI_EnabledBelow" v2="HCTAI_EnabledAbove" />
    </Variable>
    <Variable Name="Data Collection Trigger By DI Switch" DataType="H_UCHAR" Description="How to react to DI switch">
      <EnumValues v0="HCTDI_Disabled" v1="HCTDI_EnabledOpen" v2="HCTDI_EnabledClosed" />
    </Variable>
  </Variables>
</HART>