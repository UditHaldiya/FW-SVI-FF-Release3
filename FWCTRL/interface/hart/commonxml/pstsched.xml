      </SubCommands>
    </SubCmdOnly>
    <SubCmdOnly CmdNum="171">
      <SubCommands>
        <Command Name="Write Partial Stroke Test Start Config" CmdNum="196" CmdType="Write" AllowedModes="HC_ALLOWED_MODE_ALL | HC_ALLOWED_MODE_FS" CmdFlags="HC_WRITE_COMMAND" Description="Write Partial Stroke Test Start Config">
          <RequestParams>
            <Param>PST Trigger By UI</Param>
            <Param>Data Collection Trigger By HART</Param>
            <Param>Data Collection Trigger By DI Switch</Param>
            <Param>Data Collection Trigger By AI Input</Param>
            <Param>Data Collection Trigger AI Input Threshold</Param>
          </RequestParams>
          <ResponseParams>
            <Param>PST Trigger By UI</Param>
            <Param>Data Collection Trigger By HART</Param>
            <Param>Data Collection Trigger By DI Switch</Param>
            <Param>Data Collection Trigger By AI Input</Param>
            <Param>Data Collection Trigger AI Input Threshold</Param>
          </ResponseParams>
        </Command>
        <Command Name="Write Partial Stroke Test Schedule" CmdNum="194" CmdType="Write" AllowedModes="HC_ALLOWED_MODE_ALL | HC_ALLOWED_MODE_FS" CmdFlags="HC_WRITE_COMMAND" Description="Write Time Until Next Partial Stroke Test. If the time exceeds PST interval, it is still accepted but taken modulo interval.">
