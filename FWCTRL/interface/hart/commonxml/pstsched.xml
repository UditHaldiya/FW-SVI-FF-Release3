        </Command>
        <Command Name="Read Partial Stroke Test Schedule" CmdNum="194" CmdType="Read" AllowedModes="HC_ALLOWED_MODE_ALL | HC_ALLOWED_MODE_FS" CmdFlags="HC_READ_COMMAND" Description="Read Time Until Next Partial Stroke Test">
          <RequestParams></RequestParams>
