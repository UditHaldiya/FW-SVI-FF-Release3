#Specify whether position error histogram is writable (like in R2)
HISTOGRAM_OPTIONS_POSERR = WRITABLE, READONLY
HISTOGRAM_SELECTION_POSERR = READONLY

#Specify whether position error histogram is writable (like in R2, only time which clears everything)
HISTOGRAM_OPTIONS_POSITION = WRITABLE, READONLY
HISTOGRAM_SELECTION_POSITION = WRITABLE

#Specify whether working times are writable
HISTOGRAM_OPTIONS_WORKING_TIMES = WRITABLE, READONLY
HISTOGRAM_SELECTION_WORKING_TIMES = READONLY

